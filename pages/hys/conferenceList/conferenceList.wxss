  padding: 0 10rpx;
}
.hy-list{
  display: flex;
  padding: 20rpx;
  background: #fff;
  margin-top: 20rpx;
  border: 2rpx solid #dcdcdc;
  border-radius: 10rpx;
  box-shadow: 0rpx 10rpx 10rpx #dcdcdc;
}
